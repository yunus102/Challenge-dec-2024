{
    "dist":[
        {
            "src": "./app.js",
            "use": "@vercel/node"
        },
        {
        "src": "frontend/package.json",
        "use": "@vercel/static-build"
        }
    ],
    "routes": [
        {
            "src": "/.*",
            "dest": "app.js"
        }
    ]
}